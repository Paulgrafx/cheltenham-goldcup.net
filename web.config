<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="Imported Rule 1">
                    <match url="^(.*)$" ignoreCase="false" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{HTTP_HOST}" pattern="^www.cheltenham-goldcup.net$" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Redirect" url="http://www.cheltenham-goldcup.net/{R:1}" redirectType="Permanent" />
                </rule>
				<rule name="Remove Indexes" stopProcessing="true">
                    <match url="^index.php(.*)$" ignoreCase="false" />
                    <conditions logicalGrouping="MatchAll" trackAllCaptures="false" />
                    <action type="Redirect" url="http://www.cheltenham-goldcup.net/{R:1}" redirectType="Permanent" />
                </rule>
                <rule name="Imported Rule 2" stopProcessing="true">
                    <match url="^google([0-9a-z]+).html$" ignoreCase="false" />
                    <action type="Rewrite" url="google{R:1}.html" />
                </rule>
                <rule name="Imported Rule 3" stopProcessing="true">
                    <match url="^404.html" ignoreCase="false" />
                    <action type="Rewrite" url="template-404.php" />
                </rule>
                <rule name="Imported Rule 7" stopProcessing="true">
                    <match url="^sitemap.html" ignoreCase="false" />
                    <action type="Rewrite" url="template-sitemap.php" appendQueryString="true" />
                </rule>
                <rule name="Imported Rule 8" stopProcessing="true">
                    <match url="^promotions.html" ignoreCase="false" />
                    <action type="Rewrite" url="template-free-bets.php" appendQueryString="true" />
                </rule>
                <rule name="Imported Rule 9" stopProcessing="true">
                    <match url="^([a-zA-Z0-9-_]+).html$" ignoreCase="false" />
                    <action type="Rewrite" url="template-new.php?page={R:1}" appendQueryString="true" />
                </rule>
                <rule name="Imported Rule 10" stopProcessing="true">
                    <match url="^out/([0-9a-z-_]+)$" ignoreCase="false" />
                    <action type="Rewrite" url="out-template.php?bookie={R:1}" appendQueryString="true" />
                </rule>
                <rule name="Imported Rule 11" enabled="true" stopProcessing="true">
                    <match url="^/?$" ignoreCase="false" />
                    <action type="Rewrite" url="template-new.php?page=index" appendQueryString="true" />
                </rule>
            </rules>
        </rewrite>
        <httpErrors>
            <remove statusCode="404" subStatusCode="-1" />
            <error statusCode="404" prefixLanguageFilePath="" path="/404.html" responseMode="ExecuteURL" />
        </httpErrors>
        <caching>
            <profiles>
                <remove extension=".php" />
                <add extension=".php" policy="CacheForTimePeriod" kernelCachePolicy="DontCache" duration="00:02:10" varyByQueryString="page,bookie" />
            </profiles>
        </caching>
    </system.webServer>
</configuration>
